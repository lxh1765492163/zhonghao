/* pages/default/integralmall/logs/logs.wxss */

.titlebar {
      padding: 10rpx 20rpx;
      font-size: 35rpx;
      border-bottom: solid 1rpx #ddd;

}

.join {
      font-size: 35rpx;
      padding: 20rpx;
      border-bottom: solid 1rpx #ddd;
}

.join .logs {
      display: flex;
      align-items: start;
      padding: 10rpx;
      border-bottom: solid 1rpx #ddd;
}

.join .logs .info {
      margin: 0 30rpx 0 20rpx;
}

.join .logs .info .title {
      word-break: break-all;
      text-overflow: ellipsis;
      display: -webkit-box;
      -webkit-box-orient: vertical;
      -webkit-line-clamp: 2;
      overflow: hidden;
      line-height: 120%;
}
