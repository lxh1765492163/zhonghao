.position{
	width: 100%;
	height: 60rpx;
	line-height: 60rpx;
	padding: 10rpx 0;
	display: flex;
	flex-direction: row;
	justify-content: center;
	align-items: center;
	font-size: 32rpx;
	font-weight: bold;
	background: #F5F5F5;
	opacity: 0.8;
	position: fixed;
	top: 0;
	z-index: 100;
}



.position image{
	width: 35rpx;
	height: 35rpx;
	display: block;
}

.position .address{
	overflow: hidden;
	margin-left: 20rpx;
	overflow: hidden;
}

.position .address text{
	float: left;
	margin-right: 20rpx;
}

.position .address image{
	width: 35rpx;
	height: 35rpx;
	margin-top: 13rpx;
	float: left;
}