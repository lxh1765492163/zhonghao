.frame {
  height: 0;
  position: fixed;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  z-index: 999;
  opacity: 0;
  overflow: hidden;
  font-size: 40rpx;
}

.frame .mark {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
}

.frame .content {
  width: 500rpx;
  height: 230rpx;
  background: white;
  position: fixed;
  left: 50%;
  top: 50%;
  bottom: 0;
  right: 0;
  z-index: 9999;
  border-radius: 15rpx;
  margin-top: -200rpx;
  margin-left: -250rpx;
}
.frame .content1 {
    display:none;
}

.frame .content .prompt {
  text-align: center;
  height: 150rpx;
  line-height: 150rpx;
}

.frame .content .frame_btn {
  height:80rpx;
}

.frame .content .frame_btn button {
  font-size: 38rpx;
  display: inline-block;
  width: 50%;
  line-height: 80rpx;
  border-radius:0;
  background: white;
  border-top: solid 1rpx #ddd;
}

.frame .content .frame_btn button::after {
  border-radius: 0;
}
