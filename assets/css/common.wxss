.jui-icongroup{
	width: 100%;
	height: auto;
	padding: 20rpx 0;
	display: flex;
	flex-direction: row;
	justify-content: center;
	display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
	flex-wrap: wrap;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}

.jui-icongroup .row-item{
	text-align: center;
	box-sizing: border-box;
}

.jui-icongroup .row-item view.icon{
	font-size: 45rpx;
}

.jui-icongroup .row-item view.icon text.icon{
	font-size: 45rpx;
}

.jui-icongroup .border-r:not(:last-child){
	border-right: 1px solid #eee;
}

.jui-row-3 .row-item{
	width: 33.33%;
}

.jui-row-4 .row-item{
	width: 25%;
}

.jui-row-5 .row-item{
	width: 20%;
}


.jui-notice{
	width: 100%;
	height: 60rpx;
	padding: 20rpx 0;
	overflow: hidden;
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
	display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}

.jui-notice .logo{
	width: 12%;
	height: 100%;
	text-align: center;
	border-right: 1px solid #eee;
}

.jui-notice .logo image{
	width: 50rpx;
	height: 50rpx;
	display: block;
	margin: 5rpx auto 0;
}

.jui-notice .icons{
	width: 8%;
	height: 100%;
	line-height: 60rpx;
	text-align: center;
	overflow: hidden;
}

.jui-notice .icons text.icon{
	margin-top: 2rpx;
	display: block;
}

.jui-notice .notice-wrap{
	width: 75%;
	height: 100%;
	line-height: 60rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}

.jui-notice .notice-wrap .text{
	width: 100%;
	height: 100%;
	line-height: 60rpx;
}


